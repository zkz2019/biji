          }
        }
      ]
    };
  },
  watch: {
    dialogVisible(val) {
      if (val) {
        this.onRefresh();
      } else {
      }
    }
  },
  methods: {
    onClick(data) {
      this.duid = data.duid;
      this.DetailsVisible = true;
    },
    onReset() {
      this.param.search = "";
      this.onRefresh();
    },
    onRefresh() {
      this.refresh = new Date().getTime();
