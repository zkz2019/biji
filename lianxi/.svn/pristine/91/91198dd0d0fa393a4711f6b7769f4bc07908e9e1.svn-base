      DetailsVisible: false,
      param: { search: "" },
      duid: "",
      refresh: 0,
      list: [
        {
          name: "序号",
          type: "$index",
          width: "80px"
        },
        {
          name: "操作时间",
          prop: "ducdate",
          minWidth: "120px"
        },
        {
          name: "升级开始日期",
          prop: "austartdate",
          minWidth: "120px"
        },
        {
          name: "升级结束日期",
          prop: "auenddate",
          minWidth: "120px"
        },
        {
          name: "升级开始时间",
          prop: "austarttime",
          minWidth: "110px"
        },
        {
          name: "升级结束时间",
          prop: "auendtime",
          minWidth: "110px"
        },
        {
          name: "升级版本号",
          prop: "deviceversion",
