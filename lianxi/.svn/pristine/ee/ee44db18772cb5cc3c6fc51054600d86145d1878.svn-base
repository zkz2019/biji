/**Home**/
.el-menu--popup-bottom-start {
    margin: 0 !important;
}

.home {
    padding: 0;
    margin: 0;
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;

    .header {
        height: 58px;
        line-height: 58px;
        display: flex;
        background-color: #1B2437;

        .header-left {
            max-width: 220px;
            min-width: 200px;
            margin-right: 80px;

            .logo {
                max-height: 48px;
                max-width: 180px;
                vertical-align: middle;
                margin: 0 20px;
            }
        }

        .header-left_mar {
            // width: 20px;
            max-width: 220px;
            margin-right: 80px;
            min-width: 200px;
        }

        .header-content {
            text-align: left;
            flex: 1;
            white-space: nowrap;
            display: flex;
            position: relative;
            overflow: hidden;

            .header-scroll {
                width: 20px;
                position: absolute;
                top: 0;
                cursor: pointer;
                z-index: 10;
                font-size: 14px;
                padding-top: 2px;
                color: $fff-color;
                background-color: #1c2327;

                &.left {
                    left: 0;
                }

                &.right {
                    right: 0;
                }
            }

            .header-menu {
                -webkit-box-flex: 0;
                -ms-flex-positive: 0;
                flex-grow: 0;
                -ms-flex-negative: 0;
                flex-shrink: 0;
                transition: transform .3s, -webkit-transform .3s;
                // margin: 0 auto;
                display: inline-block;


                &.scroll .el-menu {
                    padding: 0 20px;
                    background: #1B2437;
                }

                .el-submenu {
                    &.is-active .el-submenu__title {
                        i {
                            color: $but-color !important;
                        }
                    }

                    .el-submenu__title {
                        font-size: 16px;

                        img {
                            margin-right: 8px;
                        }

                        i {
                            margin-right: 10px;
                            color: $fff-color !important;
                        }
                    }

                    .el-submenu__icon-arrow {
                        display: none;
                    }
                }

                .el-menu.el-menu--horizontal {
                    background: #fff;
                    border-bottom: 0 solid $bap-color;
                }

                .el-menu--horizontal>.el-menu-item {
                    font-size: 14px;
                    height: 58px;
                    line-height: 58px;
                    background: #1B2437;

                    i {
                        margin-right: 10px;
                    }
                }

                .el-submenu {
                    background: #fff;
                }

                .el-submenu__title {
                    background: #1B2437 !important;
                }

                .el-menu--horizontal>.el-submenu .el-submenu__title {
                    height: 58px;
                    line-height: 58px;
                }
            }

        }

        .quick {
            &.is-active {
                color: $but-color;
                border-bottom: 2px solid #24A6F9; // rgb(21, 126, 223);
            }

            height: 58px;
            line-height: 58px;
            margin:0 30px 0 80px;
            cursor: pointer;
            display: flex;
            color: $fff-color;
            float: left;
            font-size: 16px;
            padding: 0 10px;

            i {
                margin-right: 10px;
            }
        }

        .header-right {
            max-width: 200px;
            min-width: 120px;
            text-align: right;
            padding-right: 15px;
            position: relative;

            .el-dropdown {
                height: 58px;
            }

            .userinfo-inner {
                cursor: pointer;
                color: $fff-color;
                display: inline-block;

                // .logo {
                //     display: inline-block;
                //     width: 40px;
                //     height: 40px;
                //     border-radius: 50%;
                //     background: #fff;
                //     margin: 10px 0px 10px 10px;

                img {
                    // width: 30px;
                    // height: 30px;
                    width: 28px;
                    height: 28px;
                    border-radius: 50%;
                    margin: 10px 10px 14px 10px;
                    // margin: 5px;
                    // float: right;
                    border-radius: 50%;
                    background: #409eff; //#fff;
                }

                // }



                .userinfo-name {
                    font-size: 16px;
                    display: inline-block;
                    max-width: 115px;
                    white-space: nowrap;
                    overflow: hidden;
                    margin-right: 10px;
                }
            }

            .full-screen {
                top: 10px;
                right: 5px;
                position: absolute;
                width: 30px;
                height: 30px;
            }
        }
    }

    .home-codewarn {
        color: #ff8383;
        text-align: center;
        background-color: #ffd9d9;

        span {
            padding-right: 10px;
        }
    }

    .section {
        background-color: $bap-color;
        flex: 1;
        display: flex;
        flex-direction: column;

        .section-header {
            margin: 8px 10px;
            height: 32px;
            line-height: 32px;

            .el-tabs__item {
                &#tab-index {
                    >span {
                        padding-right: 5px;
                    }

                    span.el-icon-close {
                        display: none;
                    }
                }

                height: 32px;
                line-height: 32px;
            }

            .el-tabs--card>.el-tabs__header {
                border-bottom: none; //1px solid $ccc-color;

                .el-tabs__nav {
                    border-radius: 10px 10px 0 0;
                    border: 1px solid $ccc-color;
                    border-bottom: 0;
                    background-color: $fff-color;
                }
            }
        }

        .section-content {
            overflow: hidden;
            flex: 1;
            display: flex;
            flex-direction: column;

            >section {
                flex: 1;
                overflow: auto;
            }
        }
    }
}

