/*****房间管理******/
.roomBox_title_item {
    list-style: none;
    line-height: 36px;
    padding: 0 20px;
    margin: 0;
    font-size: 14px;
    color: #606266;
    cursor: pointer;
    outline: 0;

    &:hover {
        background-color: #ecf5ff;
        color: #66b1ff;
    }
}

.selection {
    width: 0;
    height: 0;
    display: block;
    content: "";
    z-index: 10;
    border-top: 12px solid #999999;
    border-left: 8px solid #fff;
    border-bottom: 0px solid #fff;
    border-right: 8px solid #fff;
    position: absolute;
    top: 4px;
    left: 25px;

    &.type-1,
    &.type-2,
    &.type-3,
    &.type-4,
    &.type-5,
    &.type-6 {
        border-top-color: #24a6f9;
    }

    &.type--1,
    &.type--3 {
        border-top-color: #ff4141;
    }
}

