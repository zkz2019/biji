<template>
  <el-dialog
    title="历史记录"
    width="70%"
    class="importHistory"
    append-to-body
    :close-on-click-modal="false"
    :before-close="beforeClose"
    :visible.sync="dialogVisible"
  >
    <el-container class="dialog-table6 wh100">
      <el-header class="query_headbox">
        <retrieval class="query_head">
          <inpbox inptext="请选择操作类型">
            <el-select class="qh_inp" v-model="param.ordertype" filterable placeholder="请选择">
              <el-option v-for="item in options" :key="item.id" :label="item.name" :value="item.id"></el-option>
            </el-select>
          </inpbox>
          <inpbox>
            <fel-button class="qh_btn" type="primary" @click="onRefresh">查询</fel-button>
            <fel-button class="qh_btn" @click="onReset">重置</fel-button>
          </inpbox>
        </retrieval>
      </el-header>
      <paging-table
        interface="/system/onlineup/nbversion/4/getnbversionuprecord"
        :list="listInfo"
        :param="param"
        :refresh="refresh"
      ></paging-table>
    </el-container>
  </el-dialog>
</template>

<script>
export default {
  props: {
    dialogVisible: Boolean,
    roomid: String | Number
  },
  data() {
    let $this = this;
    return {
      param: { ordertype: "", roomid: "" },
      options: [{ id: "", name: "全部" }],
      refresh: 0,
      listInfo: [
        {
          name: "序号",
          type: "$index",
          width: "60px"
        },
        {
          name: "操作类型",
          width: "90px",
          prop: "duordertypename"
        },
        {
          name: "操作开始时间",
          prop: "dudcdate",
          width: "160px"
        },
        {
          name: "操作结束时间",
          prop: "dudedate",
          width: "160px"
        },

        {
          name: "操作固件版本号",
          width: "130px",
          prop: "deviceversion"
        },
        {
          name: "操作进度",
          prop: "dudrate"
        },
        {
          name: "处理状态",
          prop: "dudstatusname",
          template: {
            props: ["scope"],
            methods: {
              getClass() {
                let value = this.scope.row.dudstatusname;
                if (value == "处理成功") {
                  return "puc-pg";
                } else if (value == "处理失败") {
                  return "puc-px";
                } else {
                  return "";
                }
              }
            },
            template: `<ul><li :class='getClass()'>{{scope.row.dudstatusname}}</li></ul>`
          }
        },
        {
          name: "指令结果",
          prop: "dudstatename",
          template: {
            props: ["scope"],
            methods: {
              getClass() {
                let value = this.scope.row.dudstatename;
                if (value == "下发成功") {
                  return "puc-pg";
                } else if (value == "下发失败") {
                  return "puc-px";
                } else {
                  return "";
                }
              }
            },
            template: `<ul><li :class='getClass()'>{{scope.row.dudstatename}}</li></ul>`
          }
        },

        {
          name: "失败原因",
          prop: "dudremark"
        },
        // {
        //   name: "备注",
        //   prop: ""
        // }
      ]
    };
  },
  computed: {},
  watch: {
    dialogVisible(val) {
      if (val) {
        this.param.roomid = this.roomid;
      } else {
        this.param.roomid = "";
      }
      this.onRefresh();
    }
  },
  created() {
    this.getType();
  },
  methods: {
    getType() {
      this.$ajax("/system/onlineup/nbversion/3/getordertype", {}, "1").then(
        res => {
          this.options = [...this.options, ...res.result];
        }
      );
    },
    beforeClose() {
      this.$emit("beforeClose");
    },
    onReset() {
      this.param.ordertype = "";
      this.onRefresh();
    },
    onRefresh() {
      this.refresh = new Date().getTime();
    }
  }
};
</script>