@media screen and (max-height: 700px) {

    .con-select {
        width: 180px;
    }

    .con-popover {
        width: 220px;
    }

    .con-search {
        width: 270px;
    }

    .dateSelect {
        width: 530px;
    }

    .con-date {
        .el-input__inner {
            width: 370px;
        }
    }

    .con-num {
        width: 140px;
    }

    //com-title
    .com-title {
        margin: 0 0 8px 0;
        padding: 12px 0 0 32px;
        height: 50px;

        span.navig {
            font-size: 17px;
        }

        .ti_text {
            height: 25px;
            font-size: 18px;
        }
    }
    .iconLogo {
        width: 24px;
        height: 52px;
        margin-right: 10px;
        fill: currentColor;
        overflow: hidden;
    }
    .home {
        .header {
            height: 52px;
            line-height: 52px;

            .header-left {
                .logo {
                    height: 36px;
                    max-width: 175px;
                }
            }

            .header-content {
                .header-menu {
                    .el-submenu {

                        .el-submenu__title {
                            font-size: 14px;
                            padding: 0 14px;

                            i {
                                font-size: 14px;
                                margin-right: 8px;
                            }
                        }
                    }

                    .el-menu--horizontal>.el-menu-item {
                        padding: 0 14px;
                        height: 52px;
                        line-height: 52px;
                        font-size: 14px;

                        i {
                            font-size: 14px;
                            margin-right: 8px;
                        }
                    }

                    .el-menu--horizontal>.el-submenu .el-submenu__title {
                        height: 52px;
                        line-height: 52px;
                    }
                }
            }

            .quick {
                height: 52px;
                line-height: 52px;
                font-size: 14px;
            }

            .header-right {
                .el-dropdown {
                    height: 52px;
                }

                .userinfo-inner {
                    img {
                        margin: 10px;
                        height: 26px;
                        width: 26px;
                    }
                }

                .full-screen {
                    top: 3px;
                }
            }
        }
    }

    .query_head {
        margin: 6px 12px;
        padding: 7px 24px;
    }

    .query_main {
        margin: 6px 12px 12px;
        padding: 0 24px;

        .paging-fy {
            padding: 16px 3px 12px;
            // padding-right: 150px;
        }
    }
}