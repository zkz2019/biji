gTableVisible' :gridData='gridData' \r\n    :rows_kp='rows_kp'\r\n    :total_kp='total_kp'\r\n    v-on:dialoghandleSize=\"dialoghandleSize\"\r\n    v-on:dialoghandleCurrent=\"dialoghandleCurrent\"\r\n    ></Dialog-Tabel>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport PublicTable from '@/components/PublicTable'    //表格\r\nimport TableHeader from '@/components/TableHeader2'   //管理卡按钮\r\nimport DialogTabel from '@/components/DialogTabel'    //已经授权表格\r\nimport RemoteJs from '@/request/RemoteJs'// 导入远程JS组件\r\nimport Cookies from 'js-cookie'\r\nexport default {\r\n    components: {\r\n    'Public-Table':PublicTable,\r\n    'Table-Header':TableHeader,\r\n    'Dialog-Tabel':DialogTabel,\r\n    'remote-js':RemoteJs\r\n   },\r\n   data () {\r\n    return {\r\n        thiscardcode:'',   //点击的表格行信息\r\n        logining:false,\r\n        pagesid:164,       //页面ID\r\n        pagesnumber:1,     //页码  \r\n        rows:15,            //每页数量    \r\n        search:'',          //搜索\r\n        total:0,        //总数\r\n        tabledata:[],      //数据//表格 \r\n        gridData:[],        //卡片授权列表 \r\n        dialogTableVisible:false,   //卡片授权列表弹框\r\n        pagesnumber_kp:15,         // 卡片授权列表每页数量\r\n        rows_kp:0,               // 卡片授权列表页码\r\n        total_kp:0,            //卡片授权列表总数\r\n        cardcode_kp:'',        //卡片授权列表卡号\r\n        logining_kp:false,\r\n    };\r\n  },\r\n    methods:{\r\n    //点击获取表格行信息\r\n    tableopenDetails(val){\r\n       this.thiscardcode=val.cardcode\r\n    },\r\n    //搜索\r\n    searchclick(val){\r\n       this.search=val\r\n    },    \r\n    dialoghandleSize(val){\r\n       this.pagesnumber_kp=val\r\n    },\r\n    dialoghandleCurrent(val){\r\n       this.rows_kp=val\r\n    },\r\n    Visible(){\r\n        this.dialogTableVisible=false\r\n    },\r\n    handle(cardcode_kp){\r\n        //卡片授权列表ID\r\n        console.log(cardcode_kp)\r\n       this.cardcode_kp = cardcode_kp\r\n       this.dialogTableVisible=true\r\n       this.gettabledata_kp()\r\n    },    \r\n    //\r\n    handleSize(e){\r\n        console.log(e)\r\n       this.rows=e\r\n    },\r\n    handleCurrent(e){\r\n       this.pagesnumber=e\r\n       console.log(e)\r\n    },\r\n    //获取表格数据\r\n    gettabledata(){\r\n        this.logining=true\r\n        let token = Cookies.get('token')\r\n        this.$post('/auth/mcard/1/getcard',{\r\n        cardstate:'',   // 0（停用）/1（正常）/2（挂失）\r\n        cardtype:'',    //3（管理卡）/4（巡更卡）/5（授权卡）\r\n        token:token,\r\n        page:this.pagesnumber,\r\n        rows:this.rows,\r\n        search:this.search,\r\n        }).then((res)=>{\r\n        this.logining=false\r\n        console.log(res.result)\r\n        this.total=res.result.total*1        //总数\r\n        this.tabledata=res.result.data      //数据\r\n         console.log(this.tabledata)\r\n        }).catch((err)=>{this.logining=false;console.log(err)})\r\n    },\r\n    //获取卡片授权列表\r\n    gettabledata_kp(){\r\n        this.logining_kp=true\r\n        let token = Cookies.get('token')\r\n        this.$post('/auth/mcard/2/getcardauth',{\r\n        token:token,\r\n        page:this.pagesnumber_kp,\r\n        rows:this.rows_kp,\r\n        cardcode :this.cardcode_kp ,\r\n        }).then((res)=>{\r\n        this.logining_kp=false\r\n        console.log(res.result)\r\n        this.total_kp=res.result.total*1        //总数\r\n        this.gridData=res.result.data      //数据\r\n        }).catch((err)=>{this.logining=false;console.log(err.data)})\r\n    },\r\n    //按钮 //远程JS  RFID读写器云服务\r\n    buttonclick(id){\r\n       this.loadRongJs()\r\n    }, \r\n    loadRongJs() {// 当使用远程js里的内容时请添加\"//eslint-disable-line\"防止eslint检测报错\r\n       try  {           \r\n            var rfidreader=YOWORFIDReader.createNew();\r\n            console.log('RFID读写器对象') //eslint-disable-line\r\n            console.log(rfidreader)//eslint-disable-line\r\n            }\r\n        catch(e)\r\n            {\r\n            alert(\"创建友我科技RFID云服务连接失败，请先下载安装！\"); //eslint-disable-line\r\n            top.location 