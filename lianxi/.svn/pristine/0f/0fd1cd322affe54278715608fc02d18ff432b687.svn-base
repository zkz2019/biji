<!-- 无线网关自动升级记录 -->
<template>
  <el-dialog
    title="无线网关自动升级记录"
    width="70%"
    class="importHistory"
    :close-on-click-modal="false"
    append-to-body
    :before-close="beforeClose"
    :visible.sync="dialogVisible"
  >
    <el-container class="dialog-table6 query_main">
      <paging-table
        interface="/system/onlineup/wxgateversionup/3/getautoversionup"
        ref="paging-table"
        :list="list"
        :param="param"
        :refresh="refresh"
      >
        <retrieval class="query_head">
          <inpbox :inpb="true">
            <el-input
              clearable
              class="qh_w270 qh_inp maR24"
              v-model="param.search"
              :placeholder="'输入网关唯一ID/位置进行查询'"
            ></el-input>
          </inpbox>
          <inpbox>
            <fel-button class="qh_btn" type="primary" @click="onRefresh">查询</fel-button>
            <fel-button class="qh_btn" @click="onReset">重置</fel-button>
          </inpbox>
        </retrieval>
      </paging-table>
    </el-container>
    <wxAutoupDetails
      :dialogVisible="DetailsVisible"
      :stopBtns="stopBtns"
      :duid="duid"
      @beforeClose="DetailsVisible=false"
    ></wxAutoupDetails>
  </el-dialog>
</template>

<script>
import wxAutoupDetails from "./wxAutoupDetails";
export default {
  props: {
    dialogVisible: Boolean,
    stopBtns: Array,
  },
  components: {
    wxAutoupDetails,
  },
  data() {
    let $this = this;
    return {
      DetailsVisible: false,
      param: { search: "" },
      duid: "",
      refresh: 0,
      list: [
        {
          name: "序号",
          type: "$index",
          width: "80px",
        },
        {
          name: "操作时间",
          prop: "ducdate",
          minWidth: "120px",
        },

        {
          name: "升级开始日期",
          prop: "austartdate",
          minWidth: "120px",
        },
        {
          name: "升级结束日期",
          prop: "auenddate",
          minWidth: "120px",
        },
        {
          name: "升级开始时间",
          prop: "austarttime",
          minWidth: "110px",
        },
        {
          name: "升级结束时间",
          prop: "auendtime",
          minWidth: "110px",
        },
        {
          name: "升级版本号",
          prop: "deviceversion",
          minWidth: "100px",
        },
        {
          name: "操作类型",
          prop: "dutype",
          minWidth: "90px",
        },
        {
          name: "总记录数",
          prop: "ducount",
          minWidth: "80px",
        },
        {
          name: "指令类型",
          prop: "duordertypename",
        },
        {
          name: "操作账号",
          prop: "userlogin",
        },
        {
          name: "操作",
          width: "100px",
          template: {
            props: ["scope"],
            methods: {
              onClick(key) {
                $this.onClick(this.scope.row);
              },
            },
            template: `<div class="operat-buts"> 
             <el-button type="text" size="small" @click.stop="onClick">详情</el-button>
            </div>`,
          },
        },
      ],
    };
  },
  watch: {
    dialogVisible(val) {
      if (val) {
        this.onRefresh();
      } else {
      }
    },
  },
  methods: {
    onClick(data) {
      this.duid = data.duid;
      this.DetailsVisible = true;
    },
    onReset() {
      this.param.search = "";
      this.onRefresh();
    },
    onRefresh() {
      this.refresh = new Date().getTime();
    },
    beforeClose() {
      this.$emit("beforeClose");
    },
  },
};
</script>
