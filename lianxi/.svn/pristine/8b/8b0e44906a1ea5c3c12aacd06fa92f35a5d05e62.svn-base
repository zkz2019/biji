<!-- 未归查询 -->
<template>
  <el-container>
    <el-header class="query_headbox">
      <retrieval class="query_head">
        <inpbox :inptext="'未归天数'">
          <el-input-number class="numlineH maR10 con-num qh_inp" v-model="param.nobackday" :min="1"></el-input-number>
        </inpbox>
        <inpbox :inptext="'请选择时间'">
          <fel-date class="maR10 qh_date con-date" style="width:370px!important;" v-model="dates"></fel-date>
        </inpbox>

        <inpbox :inptext="'选择位置'">
          <queryPosition
            ref="queryPosition"
            class="maR10 con-popover qh_inp"
            @onChoice="onChoiceWZ"
            interface="/analysis/nobackrecord/1/getbuildtree"
          ></queryPosition>
        </inpbox>

        <inpbox :inptext="'选择组织'">
          <queryOrgan
            ref="queryOrgan"
            class="maR10 con-popover qh_inp"
            @onChoice="onChoiceZZ"
            interface="/analysis/nobackrecord/2/getpersontree"
          ></queryOrgan>
        </inpbox>

        <inpbox :inptext="'请输入'">
          <el-input
            clearable
            class="search con-search qh_inp"
            v-model="param.search"
            :placeholder="'输入姓名/'+getNumber()+'进行检索'"
          ></el-input>
        </inpbox>

        <inpbox>
          <fel-button class="qh_btn" type="primary" @click="search">查询</fel-button>
          <fel-button class="qh_btn" @click="onReset">重置</fel-button>
          <!-- <fel-button class="qh_btn" type="primary" @click="exportBut">导出</fel-button> -->
        </inpbox>
      </retrieval>
    </el-header>
    <el-main class="padt0 query_main">
      <paging-table
        class="tobleList wid100 heig100"
        height="100%"
        interface="/analysis/nobackrecord/3/getnobackrecord"
        :list="list"
        :refresh="refresh"
        :param="param"
        :paramObj="paramObj"
        @onEjectChange="onEjectChange"
      >
        <template v-if="isExport">
          <span class="sli but-blue" @click="exportBut">
            <i class="ficon-export"></i>
            导出
          </span>
          <span class="sli but-blue" @click="exportHistory">
            <i class="ficon-history"></i>
            导出历史记录
          </span>
        </template>
      </paging-table>
    </el-main>
    <exportHistory
      historyUrl="/analysis/nobackrecord/6/downhistory"
      :dialogVisible="iHvisible"
      @beforeClose="()=>{iHvisible=false}"
    ></exportHistory>
  </el-container>
</template>

<script>
import Storages from "../../utils/Storage.js"; //缓存工具
import { mapGetters } from "vuex";
import { format, download } from "@/utils/utils.js";
import exportHistory from "../personnel/export-history";
import { timerDownload } from "../personnel/index.js";
import queryPosition from "./queryPosition";
import queryOrgan from "./queryOrgan";
export default {
  name: "wgcx190",
  components: {
    queryPosition,
    exportHistory,
    queryOrgan
  },
  data() {
    return {
      iHvisible: false,
      paramObj: {
        type: "1",
        arearoom: [],
        build: [],
        persontree: []
      },
      dates: [],
      param: { sdate: "", edate: "", search: "", nobackday: 2 },
      list: [
        {
          name: "序号",
          type: "$index",
          width: "60px"
        },
        {
          name: "姓名",
          prop: "personname"
        },
        {
          name: this.getNumber(),
          prop: "personcode"
        },
        {
          name: "归属组织",
          prop: "personlocation"
        },
        {
          name: "房间位置",
          prop: "roomlocation"
        },
        {
          name: "最后开门时间",
          prop: "unlockingdate"
        },
        {
          name: "连续未归天数",
          prop: "days"
        }
      ],
      refresh: 0,
      isExport: false,
      sonmenu: 0
    };
  },
  created() {
    this.inGetsonmenu();
  },
  mounted() {
    this.getEject();
  },
  methods: {
    inGetsonmenu() {
      this.$ajax("/login/home/2/getsonmenu", { fatherid: "190" }, "1")
        .then(res => {
          res.result.forEach(value => {
            let id = value.entity.id;
            let alias = value.entity.alias;
            if (id == "343") {
              this.isExport = true;
            }
          });
          this.sonmenu = 4;
        })
        .catch(err => {
          if (this.sonmenu < 3) {
            setTimeout(() => {
              this.sonmenu++;
              this.inGetsonmenu();
            }, 1000);
          }
        });
    },
    exportHistory() {
      this.iHvisible = true;
    },
    //导出
    exportBut() {
      let url = "/analysis/nobackrecord/5/exportnobackecords";
      let name = "未归查询";
      if (this.dates && this.dates.length == 2) {
        this.param.sdate = format(this.dates[0], "yyyy-MM-dd HH:mm:ss");
        this.param.edate = format(this.dates[1], "yyyy-MM-dd HH:mm:ss");
      } else {
        this.param.sdate = "";
        this.param.edate = "";
      }
      this.inExportPackage(url, name, this.param, this.paramObj);
    },
    inExportPackage(url, name, data = {}, obj = {}) {
      this.$ajax(url, data, "1", obj, true, 60000)
        .then(res => {
          this.$message({
            message: name + "文件导出已下发，文件正在生成中",
            type: "success"
          });
          timerDownload(
            res.result,
            "/analysis/nobackrecord/6/downhistory",
            this,
            name
          );
        })
        .catch(err => {
          this.$message({
            showClose: true,
            message: `[${err.resultCode}] ` + err.resultMsg,
            type: "error"
          });
        });
    },
    // inExport(url, name, data = {}, obj = {}) {
    //   this.$ajax(url, data, "8", obj, "文件导出中...", 60000)
    //     .then(res => {
    //       if (res.size) {
    //         download(res, name);
    //         this.$notify({
    //           title: "成功",
    //           message: name + "文件导出成功！",
    //           type: "success"
    //         });
    //       }
    //     })
    //     .catch(err => {
    //       this.$message.error("文件导出失败！失败原因：" + err.resultMsg);
    //     });
    // },
    //重置事件
    onReset() {
      this.dates = [];
      Object.keys(this.param).forEach(key => {
        this.param[key] = "";
      });
      this.$refs.queryPosition.onClear();
      this.$refs.queryOrgan.onClear();
      this.param.nobackday = 2;
      this.search();
    },
    ...mapGetters(["getNumber"]),
    onChoiceWZ(data, type) {
      if (type == "1") {
        this.paramObj.type = 2;
        this.paramObj.arearoom = data;
        this.paramObj.build = [];
      } else {
        this.paramObj.type = 1;
        this.paramObj.arearoom = [];
        this.paramObj.build = data;
      }
      // this.paramObj.build = data;
    },
    onChoiceZZ(data) {
      this.paramObj.persontree = data;
    },
    search() {
      if (this.dates && this.dates.length == 2) {
        this.param.sdate = format(this.dates[0], "yyyy-MM-dd HH:mm:ss");
        this.param.edate = format(this.dates[1], "yyyy-MM-dd HH:mm:ss");
      } else {
        this.param.sdate = "";
        this.param.edate = "";
      }
      this.refresh = new Date().getTime();
    },
    onEjectChange() {
      this.$common.onEjectChange(this.list, "wgcx190");
    },
    getEject() {
      this.$common.getEject(this, "list", "wgcx190");
    }
  }
};
</script>