<template>
  <el-dialog
    title="历史记录"
    width="70%"
    class="importHistory"
    append-to-body
    :close-on-click-modal="false"
    :before-close="beforeClose"
    :visible.sync="dialogVisible"
  >
    <el-container class="dialog-table6 query_main">
      <paging-table
        interface="/system/onlineup/wxgateversion/4/getgateversionuprecord"
        :list="listInfo"
        :param="param"
        :refresh="refresh"
      >
          <retrieval class="query_head">
            <inpbox inptext="请选择操作类型">
              <el-select class="qh_inp" v-model="param.ordertype" filterable placeholder="请选择">
                <el-option
                  v-for="item in options"
                  :key="item.id"
                  :label="item.name"
                  :value="item.id"
                ></el-option>
              </el-select>
            </inpbox>
            <inpbox>
              <fel-button class="qh_btn" type="primary" @click="onRefresh">查询</fel-button>
              <fel-button class="qh_btn" @click="onReset">重置</fel-button>
            </inpbox>
          </retrieval>
      </paging-table>
    </el-container>
  </el-dialog>
</template>

<script>
export default {
  props: {
    dialogVisible: Boolean,
    gatewaycode: String | Number
  },
  data() {
    let $this = this;
    return {
      param: { gatewaycode: "", ordertype: "", roomid: "" },
      options: [{ id: "", name: "全部" }],
      refresh: 0,
      listInfo: [
        {
          name: "序号",
          type: "$index",
          width: "60px"
        },
        {
          name: "操作类型",
          prop: "duordertypename"
        },
        {
          name: "操作开始时间",
          width: "160px",
          prop: "dudcdate"
        },
        {
          name: "操作结束时间",
          width: "160px",
          prop: "dudedate"
        },

        {
          name: "操作固件版本号",
          minWidth: "130px",
          prop: "deviceversion"
        },
        {
          name: "操作进度",
          prop: "dudrate"
        },
        {
          name: "处理状态",
          prop: "dudstatusname",
          template: {
            props: ["scope"],
            methods: {
              getClass() {
                let value = this.scope.row.dudstatusname;
                if (value == "处理成功") {
                  return "puc-pg";
                } else if (value == "处理失败") {
                  return "puc-px";
                } else {
                  return "";
                }
              }
            },
            template: `<ul><li :class='getClass()'>{{scope.row.dudstatusname}}</li></ul>`
          }
        },
        {
          name: "结果",
          prop: "dudstatename",
          template: {
            props: ["scope"],
            methods: {
              getClass() {
                let value = this.scope.row.dudstatename;
                if (value == "下发成功") {
                  return "puc-pg";
                } else if (value == "下发失败") {
                  return "puc-px";
                } else {
                  return "";
                }
              }
            },
            template: `<ul><li :class='getClass()'>{{scope.row.dudstatename}}</li></ul>`
          }
        },
        {
          name: "失败原因",
          prop: "dudremark"
        }
      ]
    };
  },
  computed: {},
  watch: {
    dialogVisible(val) {
      if (val) {
        this.param.gatewaycode = this.gatewaycode;
      } else {
        this.param.gatewaycode = "";
      }
      this.onRefresh();
    }
  },
  created() {
    this.getType();
  },
  methods: {
    getType() {
      this.$ajax("/system/onlineup/yxgateversion/3/getordertype", {}, "1").then(
        res => {
          this.options = [...this.options, ...res.result];
        }
      );
    },
    beforeClose() {
      this.$emit("beforeClose");
    },
    onReset() {
      this.param.ordertype = "";
      this.onRefresh();
    },
    onRefresh() {
      this.refresh = new Date().getTime();
    }
  }
};
</script>