                // value: "amstate",
                name: "门禁状态",
                render(obj, add) {
                  if (obj.amstate == "在线") {
                    return add("div", { class: "puc-pg" }, obj.amstate);
                  } else {
                    return add("div", { class: "puc-px" }, obj.amstate);
                  }
                }
              },
              {
                value: "",
                name: "消防报警功能状态"
              },
              // {
              //   value: "",
              //   name: ""
              // },
              {
                value: "ambdcards",
                name: "卡片授权数量"
              },
              {
                value: "ambdpasss",
                name: "密码授权数量"
