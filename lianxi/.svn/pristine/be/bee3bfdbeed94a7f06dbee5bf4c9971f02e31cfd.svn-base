            res.result.amwarn = "";
          }
          this.selects.doors = res.result.doors;
          this.selects.amsyncs = res.result.amsyncs;
          this.selects.amsyncauths = res.result.amsyncauths;
          this.defaultDataGNPZ = res.result;
          this.door = this.defaultDataGNPZ.door.map(item => {
