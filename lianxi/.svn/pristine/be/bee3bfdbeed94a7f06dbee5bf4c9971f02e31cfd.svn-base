            this.defaultData = data;
            this.defaultData.ambdcards = data.ambdcard + "/" + data.ammaxcard;
            this.defaultData.ambdpasss = data.ambdpass + "/" + data.ammaxpass;
            this.loading = false;
          })
          .catch(err => {
            this.loading = false;
            console.log("err", err);
          });
      }
