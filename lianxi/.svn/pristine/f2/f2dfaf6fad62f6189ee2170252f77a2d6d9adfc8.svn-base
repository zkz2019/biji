@media screen and (max-height: 520px) {

    .con-select {
        width: 140px;
    }

    .con-popover {
        width: 164px;
    }

    .con-search {
        width: 190px;
    }


    .con-date {
        .el-input__inner {
            width: 300px;
        }
    }

    .con-num {
        width: 120px;
    }



    .com-but-small {
        height: 28px;
        padding: 6px 12px;
        font-size: 12px;
        border-radius: 3px;
    }

    .el-dialog__body {
        padding: 14px 24px;
    }

    .el-dialog__header {
        padding: 15px 20px 8px
    }

    .el-dialog__footer {
        padding: 14px 24px;
    }

    .el-tabs__nav-next,
    .el-tabs__nav-prev {
        line-height: 30px;
    }

    .iconLogo {
        width: 20px;
        height: 40px;
    }

    .Home_submenu {
        .el-menu-item {
            font-size: 12px;
            padding-left: 10px !important;
        }
    }

    .el-tabs__nav-next,
    .el-tabs__nav-prev {
        line-height: 32px;
    }


    .login {

        .loginbox {
            margin-top: -160px;
            width: 310px;
            height: 320px;
            padding: 20px;

            .login-container {
                .el-form-item {
                    height: 34px;
                    line-height: 34px;

                    .el-form-item__label {
                        height: 34px;
                        line-height: 34px;
                    }

                    .el-form-item__content {
                        line-height: 34px;

                        input {
                            height: 34px;
                            line-height: 34px;
                        }
                    }
                }

                .top {
                    font-size: 20px;
                    margin-top: 0;
                    margin-bottom: 15px;
                }

                .checkboxa {
                    margin-bottom: 10px;
                }

                .form-test {

                    .el-form-item__content {

                        .login-img {
                            margin-left: 6px;
                            width: 66px;
                            height: 34px;

                            .el-image {
                                height: 34px;
                            }

                        }
                    }
                }

                .login_but {
                    padding: 0;
                    height: 34px;
                    line-height: 32px;
                }
            }
        }
    }

    .check-login-dialog {
        width: 288px;

        .check-login-content {
            width: 240px;
            padding: 8px 18px;

            .check-login-top {
                padding: 0 30px;
                margin-bottom: 8px;

                i {
                    width: 40px;
                    height: 40px;
                    background-size: 40px 40px;
                }

                span {
                    font-size: 11px;
                }
            }

            .check-login-pwd {
                padding: 0;

                .el-form {
                    .el-input {
                        font-size: 12px;

                        input {
                            height: 32px;
                        }
                    }

                }
            }

            .check-login-buts {
                .el-button {
                    width: 76px;
                }
            }

        }
    }

    .home {
        .header {
            height: 40px;
            line-height: 40px;

            .header-left {
                .logo {
                    height: 28px;
                    max-width: 160px;
                }
            }

            .header-content {
                .header-menu {
                    .el-submenu {

                        .el-submenu__title {
                            padding: 0 10px;
                            font-size: 14px;

                            i {
                                font-size: 14px;
                                margin-right: 5px;
                            }
                        }
                    }

                    .el-menu--horizontal>.el-menu-item {
                        padding: 0 10px;
                        font-size: 14px;
                        height: 40px;
                        line-height: 40px;

                        i {
                            font-size: 14px;
                            margin-right: 5px;
                        }
                    }

                    .el-menu--horizontal>.el-submenu .el-submenu__title {
                        height: 40px;
                        line-height: 40px;
                    }
                }

                .header-scroll {
                    width: 20px;
                    font-size: 16px;
                    padding-top: 2px;

                }

            }

            .quick {
                height: 40px;
                line-height: 40px;
            }

            .header-right {
                .el-dropdown {
                    height: 40px;
                }

                .userinfo-inner {
                    // .logo {
                    //     margin: 4px 0px 4px 10px;
                    //     height: 32px;
                    //     width: 32px;

                    img {
                        margin: 8px; //4px 0px 4px 10px;
                        height: 24px;
                        width: 24px;
                        // margin: 3px;
                        // height: 26px;
                        // width: 26px;
                    }

                    // }

                }

                .full-screen {
                    top: 0px;
                }
            }
        }

        .section {
            .section-header {
                margin: 6px 10px;
                height: 24px;
                line-height: 24px;

                .el-tabs__item {
                    font-size: 14px;
                    height: 24px;
                    line-height: 24px;

                    i {
                        font-size: 14px;
                    }
                }
            }
        }
    }

    .com-title {
        margin: 0 0 6px 0;
        padding: 4px 0 0 32px;
        height: 32px;

        span.navig {
            font-size: 15px;
        }

        .ti_text {
            height: 20px;
            font-size: 16px;
        }

        .ti_textL {
            .el-button--mini {
                margin-top: -2px;
                padding: 4px 15px;
            }
        }
    }

    .query_head {
        margin: 4px 8px;
        padding: 4px 8px;

    }

    .inpbox {

        .qh_text {
            padding-right: 4px;
        }

        .sbox {
            margin: 2px 16px 2px 0;
        }

    }

    .query_main {
        margin: 2px 8px 8px;
        padding: 0 14px;

        .paging-fy {
            padding: 4px 3px 2px;
            // padding-right: 100px;
        }
    }

    .query_main.batch {
        margin-top: 0px;
        height: 100%; //批量授权处修改
    }

    .batch-authori {

        .b-left {
            .query-input {
                margin-bottom: 8px;

                .in-search {
                    padding: 8px;
                }
            }
        }

        .b-centre {
            margin: 0 4px;
        }

        .b-right {
            .tobleList {
                padding-bottom: 8px;
            }
        }

        .tobleList {
            padding: 0 8px;
        }

        .tsTop {
            height: 28px;
            line-height: 28px;
        }
    }

    .single-row .form-content .form-el {
        width: 80%;
    }

    .fel-form .form-content .content-div {
        margin: 0 20px 0 10px;
    }

    .fel-form .footerButton {
        padding: 14px 24px 0 24px;
    }

    .single-row .form-content .form-el {
        width: 90%;
    }


    .fel-tables .el-table td {
        padding: 2px 0;
    }

    .el-table {
        font-size: 12px;
    }

    .fel-tables .top {
        height: 32px;
    }

    .el-pagination button,
    .el-pager li,
    .el-pagination__jump input,
    .el-pagination__sizes .el-input .el-input__inner,
    .el-pagination span:not([class*=suffix]) {
        font-size: 12px;
    }
}