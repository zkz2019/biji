<!-- 修改版本:添加版本 -->
<template>
  <el-dialog
    :title="title"
    :width="wid"
    v-loading="loading"
    :element-loading-text="'请求中...'"
    element-loading-spinner="el-icon-loading"
    element-loading-background="rgba(0, 0, 0, 0.8)"
    :close-on-click-modal="false"
    :before-close="beforeClose"
    :visible.sync="dialogVisible"
  >
    <fel-form
      ref="felForm"
      :selects="selects"
      @submitForm="submitForm"
      @closeForm="beforeClose"
      @exportFile="exportFile"
      width="140px"
      dynamic
      :defaultData="defaultData"
      :formData="formData"
      @input="onInput"
    ></fel-form>
  </el-dialog>
</template>

<script>
export default {
  props: {
    options: Array,
    dvid: String | Number,
    types: String,
    dialogVisible: Boolean
  },
  data() {
    return {
      loading: false,
      selects: {
        options: []
      },
      defaultData: {},
      file: null,
      title: "",
      wid: "",
      formData: [
        {
          noShow: false,
          width: "50%",
          value: "deviceversion",
          name: "版本号",
          type: "text",
          rules: {
            required: true,
            message: "请先填写版本号",
            trigger: "blur"
          }
        },
        {
          noShow: false,
          width: "50%",
          value: "dvbei",
          name: "版本更新内容",
          type: "text",
          rules: {
            required: true,
            message: "请先填写版本更新内容",
            trigger: "blur"
          }
        },
        // {
        //   noShow: false,
        //   width: "50%",
        //   value: "dvcheck",
        //   name: "校验字节",
        //   type: "text",
        //   rules: [
        //     {
        //       required: true,
        //       message: "请先填写校验字节",
        //       trigger: "blur"
        //     },
        //     {
        //       validator: (rule, val, cb) => {
        //         let reg = /^[0-9a-fA-F]{1,4}$/ ///^[0-9]*$/g;
        //         if (reg.test(val)) {
        //           cb();
        //         } else {
        //           cb(new Error("请填写正确校验字节"));
        //         }
        //       }
        //     }
        //   ]
        // },
        {
          noShow: false,
          width: "50%",
          value: "type",
          name: "设备类型",
          type: "select",
          select: "options",
          slabel: "name",
          svalue: "id",
          rules: {
            required: true,
            message: "请先选择设备类型",
            trigger: "change"
          }
        },
        {
          noShow: false,
          width: "50%",
          value: "file",
          name: "上传文件",
          type: "uploadImg",
          uWidth: "120px",
          ulabel: "上传文件",
          isCustom: true
        }
      ]
    };
  },
  watch: {
    dialogVisible(val) {
      console.log("ddddd", this.types, "dvid", this.dvid);
      if (val) {
        if (this.types == "2") {
          this.getversioninfo();
          this.title = "修改版本";
          this.wid = "35%";
          this.formData[0].noShow = true;
          this.formData[2].noShow = true;
          this.formData[3].noShow = true;
          this.formData[1].width = "100%";
          // this.formData[2].width = "100%";
        } else if (this.types == "3") {
          this.title = "重传程序";
          this.wid = "30%";
          this.formData.forEach((item, ind) => {
            if (ind != 3) {
              item.noShow = true;
            } else {
              item.width = "100%";
            }
          });
        }
        this.selects.options = this.options;
      } else {
        this.wid = "50%";
        this.formData.forEach(item => {
          this.title = "添加版本";
          item.noShow = false;
          item.width = "50%";
        });
      }
    }
  },
  methods: {
    getversioninfo() {
      this.$ajax(
        "/system/onlineup/updateversion/1/getversioninfo",
        { dvid: this.dvid },
        "1"
      )
        .then(res => {
          this.defaultData = res.result;
        })
        .catch(err => {
          console.log("err", err);
        });
    },
    onInput(data) {
      this.file = null;
    },
    exportFile(data) {
      this.file = data;
    },
    submitForm(data) {
      if (this.types == "2") {
        let obj = { dvbei: data.dvbei, dvcheck: data.dvcheck, dvid: this.dvid };
        this.$ajax("/system/onlineup/updateversion/2/updateversion", obj, "1")
          .then(res => {
            this.$message({
              type: "success",
              message: "修改成功!"
            });
          })
          .catch(err => {
            this.$message({
              showClose: true,
              message: `[${err.resultCode}] ` + err.resultMsg,
              type: "error"
            });
          });
      } else if (this.types == "3") {
        if (!this.file) {
          this.$message({
            type: "error",
            message: "请先选择上传文件!"
          });
          return;
        }
        this.$ajax(
          "/system/onlineup/updateversion/3/updateversionorder",
          { dvid: this.dvid },
          "1",
          this.file
        )
          .then(res => {
            this.$message({
              type: "success",
              message: "重传成功!"
            });
            this.beforeClose();
          })
          .catch(err => {
            this.$message({
              showClose: true,
              message: `[${err.resultCode}] ` + err.resultMsg,
              type: "error"
            });
          });
      } else {
        delete data.file;
        if (!this.file) {
          this.$message({
            type: "error",
            message: "请先选择上传文件!"
          });
          return;
        }
          this.$ajax(
            "/system/onlineup/saveversion/1/saveversion",
            data,
            "7",
            this.file,
            120000
          )
            .then(res => {
              this.$message({
                type: "success",
                message: "添加成功!"
              });
              this.beforeClose();
            })
            .catch(err => {
              this.$message({
                showClose: true,
                message: `[${err.resultCode}] ` + err.resultMsg,
                type: "error"
              });
            });
        }
    },
    beforeClose() {
      if (this.$refs["felForm"]) {
        this.$refs["felForm"].resetForm();
      }
      this.file = null;
      this.$emit("beforeClose");
    }
  }
};
</script>

