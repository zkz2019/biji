@media screen and (max-height: 640px) {
    .con-select {
        width: 150px;
    }

    .con-popover {
        width: 190px;
    }

    .con-search {
        width: 230px;
    }


    .con-date {
        .el-input__inner {
            width: 340px;
        }
    }

    .con-num {
        width: 130px;
    }

    .com-but-small {
        height: 28px;
        padding: 6px 12px;
        font-size: 12px;
        border-radius: 3px;
    }

    .el-dialog__body {
        padding: 14px 24px;
    }

    .el-dialog__header {
        padding: 15px 20px 8px
    }

    .el-dialog__footer {
        padding: 14px 24px;
    }

    .iconLogo {
        width: 22px;
        height: 48px;
    }

    .Home_submenu {
        .el-menu-item {
            font-size: 14px;
            padding-left: 18px !important;
        }
    }

    .el-tabs__nav-next,
    .el-tabs__nav-prev {
        line-height: 32px;
    }


    .login {

        .loginbox {
            margin-top: -180px;
            width: 340px;
            height: 360px;
            padding: 20px;

            .login-container {
                .el-form-item {
                    height: 38px;
                    line-height: 38px;

                    .el-form-item__label {
                        height: 38px;
                        line-height: 38px;
                    }

                    .el-form-item__content {
                        line-height: 38px;

                        input {
                            height: 38px;
                            line-height: 38px;
                        }
                    }
                }

                .top {
                    margin-top: 4px;
                    font-size: 22px;
                    margin-bottom: 20px;
                }

                .checkboxa {
                    margin-bottom: 15px;
                }

                .form-test {

                    .el-form-item__content {

                        .login-img {
                            margin-left: 8px;
                            width: 80px;
                            height: 38px;

                            .el-image {
                                height: 38px;
                            }

                        }
                    }
                }

                .login_but {
                    padding: 0;
                    height: 38px;
                    line-height: 36px;
                }
            }
        }
    }

    .check-login-dialog {
        width: 328px;

        .check-login-content {
            width: 280px;
            padding: 10px 20px;

            .check-login-top {
                padding: 0 40px;
                margin-bottom: 10px;

                i {
                    width: 50px;
                    height: 50px;
                    background-size: 50px 50px;
                }

                span {
                    font-size: 13px;
                }
            }

            .check-login-pwd {
                padding: 2px;

                .el-form {
                    .el-input {
                        font-size: 14px;

                        input {
                            height: 34px;
                        }
                    }

                }
            }

            .check-login-buts {
                .el-button {
                    width: 90px;
                }
            }

        }
    }

    .home {
        .header {
            height: 48px;
            line-height: 48px;

            .header-left {
                .logo {
                    height: 34px;
                    max-width: 170px;
                }
            }

            .header-content {
                .header-menu {


                    .el-menu--horizontal>.el-menu-item {
                        height: 48px;
                        line-height: 48px;

                    }

                    .el-menu--horizontal>.el-submenu .el-submenu__title {
                        height: 48px;
                        line-height: 48px;
                    }
                }
            }

            .quick {
                height: 48px;
                line-height: 48px;
            }

            .header-right {
                .el-dropdown {
                    height: 48px;
                }

            }
        }

        .section {
            .section-header {
                margin: 6px 10px;
                height: 28px;
                line-height: 28px;

                .el-tabs__item {
                    font-size: 14px;
                    height: 28px;
                    line-height: 28px;

                    i {
                        font-size: 14px;
                    }
                }
            }
        }
    }

    //com-title
    .com-title {
        margin: 0 0 6px 0;
        padding: 8px 0 0 32px;
        height: 40px;

        span.navig {
            font-size: 15px;
        }

        .ti_text {
            height: 22px;
            font-size: 16px;
        }

        .ti_textL {
            .el-button--mini {
                margin-top: -2px;
                padding: 5px 15px;
            }
        }
    }

    .query_head {
        margin: 6px 8px;
        padding: 6px 16px;

    }

    .inpbox {
        .qh_text {
            padding-right: 6px;
        }

        .sbox {
            margin: 3px 20px 3px 0;
        }

    }

    .query_main {
        margin: 2px 8px 8px;
        padding: 0 16px;

        .paging-fy {
            padding: 8px 3px 4px;
            // padding-right: 120px;
        }
    }

    .query_main.batch {
        margin-top: 0px;
    }

    .batch-authori {

        .b-left {
            .query-input {
                margin-bottom: 8px;

                .in-search {
                    padding: 8px;
                }
            }
        }

        .b-centre {
            margin: 0 4px;
        }

        .b-right {
            .tobleList {
                padding-bottom: 8px;
            }
        }

        .tobleList {
            padding: 0 8px;
        }

        .tsTop {
            height: 30px;
            line-height: 30px;
        }
    }

    .single-row .form-content .form-el {
        width: 80%;
    }

    .fel-form .form-content .content-div {
        margin: 0 20px 0 15px;
    }

    .fel-form .footerButton {
        padding: 14px 24px 0 24px;
    }
}