<template>
  <el-dialog
    title="指令结果查询"
    width="70%"
    class="instructDialog"
    :close-on-click-modal="false"
    :before-close="beforeClose"
    :visible.sync="dialogVisible"
  >
    <el-container class="dialog-table6 wh100">
      <paging-table
        interface="/system/device/devicelock/order/2/getlockorder"
        :list="list"
        :param="paramObj"
        :refresh="refresh"
      />
    </el-container>
    <el-dialog
      title="门锁状态/网关路由指令明细查询"
      width="70%"
      class="instructDialog"
      append-to-body
      :close-on-click-modal="false"
      :before-close="beforeDetailed"
      :visible.sync="dialogDetailed"
    >
      <el-container class="dialog-table6 wh100">
        <paging-table
          interface="/system/device/devicelock/order/3/getlockorderinfo"
          :list="detailedList"
          :param="detailedParam"
          :refresh="detailedRefresh"
        >
          <span class="sli but-blue" @click="inReloadgatewayorder">重载未生效指令</span>
        </paging-table>
      </el-container>
    </el-dialog>
    <el-dialog
      title="强制变更明细查询"
      width="70%"
      class="instructDialog"
      append-to-body
      :close-on-click-modal="false"
      :before-close="beforeChange"
      :visible.sync="dialogChange"
    >
      <el-container class="dialog-table wh100">
        <paging-table
          style="height:100%"
          interface="/system/device/devicelock/order/4/getlockorderchangeinfo"
          :list="changeList"
          :param="detailedParam"
          :refresh="changeRefresh"
        ></paging-table>
      </el-container>
    </el-dialog>
  </el-dialog>
</template>

<script>
export default {
  props: {
    dialogVisible: Boolean,
    param: Object
  },
  data() {
    let $this = this;
    return {
      dialogChange: false,
      changeList: [
        //强制变更明细查询 列表
        {
          name: "序号",
          type: "$index",
          width: "60px"
        },
        {
          name: "门锁通讯ID",
          prop: "roomcode"
        },
        {
          name: "门锁唯一ID",
          prop: "roomcode2"
        },
        {
          name: "门锁位置",
          prop: "roomlocation",
          width: "200px"
        },
        {
          name: "强制变更前门锁状态",
          template: {
            props: ["scope"],
            template: `<span :class='scope.row.roomstatus=="通讯正常"?"puc-ps":"puc-px"'>{{scope.row.roomstatus}}</span>`
          }
        }
      ],
      changeRefresh: 0,
      detailedList: [
        {
          name: "序号",
          type: "$index",
          width: "60px"
        },
        {
          name: "网关/门锁通讯ID",
          prop: "gatewaycode"
        },
        {
          name: "网关/门锁唯一ID",
          prop: "gatewaycode2"
        },
        {
          name: "门锁位置",
          prop: "gatewaylocation"
        },
        {
          name: "状态",
          prop: "issend"
        },
        {
          name: "下发日期",
          prop: "sigcdate"
        },
        {
          name: "返回日期",
          prop: "sigedate"
        },
        {
          name: "备注",
          prop: "failtype"
        }
      ],
      detailedParam: {
        goid: ""
      },
      detailedRefresh: 0,
      dialogDetailed: false,

      refresh: 0,
      list: [
        {
          name: "序号",
          type: "$index",
          width: "60px"
        },
        {
          name: "范围",
          prop: "gocontent"
        },
        {
          name: "数量",
          prop: "gocount",
          template: {
            props: ["scope"],
            methods: {
              isNumber(num) {
                if (num) {
                  return /^[0-9]*$/.test(num) && num != 0;
                }
                return false;
              },
              onClick() {
                if (this.scope.row.goid) {
                  $this.detailedParam.goid = this.scope.row.goid;
                  if (
                    this.scope.row.gotype2 == "强制变更为未安装" ||
                    this.scope.row.gotype2 == "强制变更为通讯正常"
                  ) {
                    $this.onChangeRefresh();
                    $this.dialogChange = true;
                  } else {
                    $this.onDetailedRefresh();
                    $this.dialogDetailed = true;
                  }
                }
              }
            },
            template: `<div>
              <span v-if="isNumber(scope.row.gocount)"><a class="a-click" @click.stop="onClick">{{scope.row.gocount}}</a></span>
              <span v-else>{{scope.row.gocount}}</span>
            </div>`
          }
        },
        {
          name: "范围类型",
          prop: "gotype"
        },
        {
          name: "指令类型",
          prop: "gotype2"
        },
        {
          name: "操作人",
          prop: "userlogin"
        },
        {
          name: "操作日期",
          prop: "godate"
        }
      ]
    };
  },
  computed: {
    paramObj() {
      return {
        search:""// this.param.gotype2
      };
    }
  },
  watch: {
    dialogVisible(val) {
      if (val) {
        this.onRefresh();
      }
    }
  },
  methods: {
    onChangeRefresh() {
      this.changeRefresh = new Date().getTime();
    },
    beforeChange() {
      this.dialogChange = false;
    },
    inReloadgatewayorder() {
      if (this.detailedParam.goid) {
        this.$ajax(
          "/system/device/devicelock/order/5/reloadlockorder",
          { goid: this.detailedParam.goid },
          "1",
          {},
          true
        )
          .then(res => {
            this.onDetailedRefresh();
            this.$message({
              message: "重载未生效指令成功",
              type: "success"
            });
          })
          .catch(err => {
            this.$message({
              showClose: true,
              message: `[${err.resultCode}] `+err.resultMsg ,
              type: "error"
            });
          });
      }
    },
    onDetailedRefresh() {
      this.detailedRefresh = new Date().getTime();
    },
    beforeDetailed() {
      this.dialogDetailed = false;
    },
    onRefresh() {
      this.refresh = new Date().getTime();
    },
    beforeClose() {
      this.$emit("beforeClose");
    }
  }
};
</script>

