  padding: 10px 50px;
  .selBox {
    width: 100%;
    // margin: 10px 150px;
  }
  .butBox {
    // border-top: 1px solid #ccc;
    // margin: 10px 150px;
    height: 50px;
    button {
      float: right;
      margin-left: 10px;
    }
  }
}
